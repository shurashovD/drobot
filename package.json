{
  "name": "drobot",
  "version": "1.0.0",
  "description": "Olga Drobot's permanent makeup evolutuion system",
  "main": "app.js",
  "scripts": {
    "admin": "npm run start --prefix admin",
    "admin:install": "npm install --prefix admin",
    "admin:build": "npm run build --prefix admin",
    "admin-dev": "concurrently \"npm run server\" \"npm run admin\"",
    "evalution": "npm run start --prefix evalution",
    "evalution:install": "npm install --prefix evalution",
    "evalution:build": "npm run build --prefix evalution",
    "evalution-dev": "concurrently \"npm run server\" \"npm run evalution\"",
    "participants": "npm run start --prefix participants",
    "participants:install": "npm install --prefix participants",
    "participants:build": "npm run build --prefix participants",
    "participants-dev": "concurrently \"npm run server\" \"npm run participants\"",
    "start": "cross-env NODE_ENV=production node app",
    "server": "nodemon app.js"
  },
  "keywords": [
    "node",
    "express",
    "mongo",
    "react"
  ],
  "author": "Dmitry Shurashov <shurashovD@uandex.ru>",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "bootstrap": "^5.1.1",
    "cloudconvert": "^2.2.4",
    "config": "^3.3.6",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "express-handlebars": "^5.3.4",
    "md5": "^2.3.0",
    "mongoose": "^6.0.4",
    "multer": "^1.4.3",
    "node-sass": "^6.0.1",
    "node-telegram-bot-api": "^0.54.0",
    "react-bootstrap": "^2.0.0-rc.1"
  }
}
